{
	"variables": {
		"triton_account": "",
		"triton_key_id": ""
	},
	"builders": [{
		"type": "triton",
		"triton_account": "{{user `triton_account`}}",
		"triton_key_id": "{{user `triton_key_id`}}",
		"source_machine_name": "manta-ci-bridge-image-builder",
		"source_machine_package": "g4-highcpu-128M",
		"source_machine_image_filter": {
			"name": "base-64",
			"type": "zone-dataset",
			"most_recent": "true",
			"os": "smartos"
		},
		"communicator": "ssh",
		"ssh_username": "root",
		"image_name": "manta-ci-bridge",
		"image_version": "1.0.0"
	}],
	"provisioners": [{
		"type": "shell",
		"script": "provision.sh"
	}]
}
